{
    "public": true,
    "cleanUrls": true,
    "headers": [
      {
        "source": "/Songs/(.*)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=0, must-revalidate"
          },
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          }
        ]
      }
    ],
    "routes": [
      {
        "src": "/Songs/(.*)",
        "dest": "/Songs/$1",
        "status": 200
      }
    ]
  }
  